{
  "tls": {
    "version": "1.3",
    "cert_path": "/etc/ssl/certs/production.crt",
    "key_path": "/etc/ssl/private/production.key",
    "ca_path": "/etc/ssl/certs/ca-bundle.crt",
    "protocols": [
      "TLSv1.2",
      "TLSv1.3"
    ],
    "cipher_suites": [
      "TLS_AES_256_GCM_SHA384",
      "TLS_CHACHA20_POLY1305_SHA256",
      "TLS_AES_128_GCM_SHA256"
    ]
  }
}